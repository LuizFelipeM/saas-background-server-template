{
  "name": "st-background-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch --include 'src/**/*.ts' ./src/server.ts",
    "dev:local": "tsx watch --env-file=./.env --include 'src/**/*.ts' ./src/server.ts",
    "build": "tsc",
    "start": "node build/server.js",
    "prisma:db:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "commit": "cz",
    "commit:all": "git add . && cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@bull-board/api": "^6.11.0",
    "@bull-board/express": "^6.11.0",
    "@prisma/client": "^6.11.1",
    "@saas-packages/database-manager": "^1.2.0",
    "@saas-packages/queue-manager": "^1.5.0",
    "bullmq": "^5.56.2",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.6.1",
    "prisma": "^6.12.0",
    "reflect-metadata": "^0.2.2",
    "stripe": "^18.3.0",
    "tsx": "^4.20.3",
    "tsyringe": "^4.10.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.6",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
